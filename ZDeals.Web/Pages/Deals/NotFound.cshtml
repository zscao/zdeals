@page
@model ZDeals.Web.Pages.Deals.NotFoundModel
@{
    ViewData["Title"] = "Deal Not Found";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}


@await Component.InvokeAsync("BreadCrumb")

<!-- shop area start -->
    <div class="shop_area">
        <div class="container">

            <div class="row">
                <!--sidebar start-->
                <div class="col-lg-3 col-md-12">
                    @await Component.InvokeAsync("MainCategory")
                </div>
                <!--sidebar end-->
                <!-- product list start -->
                <div class="col-lg-9 col-md-12">
                    <!--shop wrapper start-->
                    <div class="row no-gutters shop_wrapper">
                        <div class="col-12">
                            <article>
                                <figure class="row">
                                    <div class="col deal_list">
                                        Oops! The deal is not available at the moment!
                                    </div>
                                </figure>
                            </article>
                        </div>

                    </div>
                    <!--shop wrapper end-->
                </div>
                <!-- product list end -->
            </div>
        </div>
    </div>
<!-- shop area end -->