{
  "ConnectionStrings": {
    "ZDealsLocal": "Server=localhost; Database=zdeals; User Id=test; Password=Test123!"
  },
  "CorsOptions": {
    "AllowedOrigins": "http://localhost:3000,http://ubuntu2004,http://fatdeals.com.au"
  },
  "PathBase": "/webapi/",
  "PictureStorageOptions": {
    "GetPictureUrl": "http://fatdeals.com.au/picture/pictures"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "/var/www/zdeals/logs/log-.txt",
            "rollingInterval": "Day",
            "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss zzz} [{Level:u3}] {SourceContext}{NewLine}{Message:lj}{NewLine}{Exception}{NewLine}"
          }
        }
      ]
    }
}
